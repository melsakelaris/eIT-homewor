import UIKit

func brackets(_s: String)-> Bool {
    var stack: [Character] = []
    let matches: [Character:Character] = ["(":")", "[":"]", "{":"}"]
    
    for char in _s {
        if let match = matches[char] {
            stack.append(match)
        }
        else if stack.last == char {
            stack.popLast()
        }
        else {
            return false
        }
    }
    return stack.isEmpty
}

print(brackets(_s: "(())"))
print(brackets(_s: "{{)"))
